{
  "name": "@rustic-enum/plus",
  "version": "0.0.0",
  "private": true,
  "description": "Rust's Option and Result functionality in TypeScript via rustic-enum",
  "license": "MIT",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "vitest",
    "dev": "vitest",
    "build": "tsup src --format esm,cjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "cov": "vitest run --coverage",
    "cov:watch": "vitest run --coverage",
    "cov:detail": "vitest run --coverage && open-cli index.html",
    "lint": "tsc && eslint src --ext .ts && prettier --no-config --check 'src/**/*.ts'",
    "format": "tsc && eslint src --fix --ext .ts && prettier --no-config --write 'src/**/*.ts'",
    "uncache": "rm -rf .turbo",
    "clear": "rm -rf .turbo && rm -rf dist && rm -rf coverage",
    "clean": "pnpm clear && rm -rf node_modules"
  },
  "dependencies": {
    "@rustic-enum/core": "workspace:0.0.0"
  }
}
